<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      .input-group-id {
        display: none;
      }
    </style>
  </head>
  <body>
    <div class="form">
      <!-- <%=JSON.stringify(EditUser) %> -->
      <form action="/udate-user" method="POST">
        <fieldset>
          <legend>Personalia:</legend>
          <div class="input-group-id">
            <label>ID:</label>
            <input name="UserId" value="<%=EditUser.id%>" /><br /><br />
          </div>
          <div class="input-group">
            <label for="email">Email:</label>
            <input
              type="email"
              id="email"
              name="email"
              value="<%=EditUser.Email%>"
            /><br /><br />
          </div>
          <div class="input-group">
            <label for="lname">Last name:</label>
            <input
              type="text"
              name="lname"
              value="<%=EditUser.LName%>"
            /><br /><br />
          </div>

          <div class="input-group">
            <label for="fname">First name:</label>
            <input
              type="text"
              name="fname"
              value="<%=EditUser.FName%>"
            /><br /><br />
          </div>

          <div class="input-group">
            <label for="adress">Address:</label>
            <input
              type="text"
              name="adress"
              value="<%=EditUser.Address%>"
            /><br /><br />
          </div>
          <div class="input-group">
            <label>City:</label>
            <input
              type="email"
              name="city"
              value="<%=EditUser.City%>"
            /><br /><br />
          </div>
          <button class="Btn-Save">Save</button>
        </fieldset>
      </form>
    </div>
  </body>
</html>
